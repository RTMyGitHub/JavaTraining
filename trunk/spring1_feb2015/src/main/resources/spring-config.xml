<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
	
<bean id="vehicle" class="com.training.spring.Car"></bean>

<bean id="traveller" class="com.training.spring.Traveller">
	<constructor-arg ref="vehicle"></constructor-arg>
</bean>

<bean id="traveller2" class="com.training.spring.Traveller2">
	<property name="vehicle" ref="vehicle"></property>
	<property name="name" value="Anteneh"></property>
</bean>

</beans>
